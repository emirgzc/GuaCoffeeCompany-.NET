
@model IEnumerable<EntityLayer.Concrete.Sube>

@{
	ViewBag.Title = "Index";
	Layout = "~/Views/Shared/_AnaLayout.cshtml";
}

<section class="page-header">
	<div class="container">
		<div class="row">
			<div class="col-md-12">
				<div class="content">
					<h1 class="page-name">ŞUBELERİMİZ</h1>
					<ol class="breadcrumb">
						<li><a href="/Slider/Index/">Anasayfa</a></li>
						<li class="active">Şubelerimiz</li>
					</ol>
				</div>
			</div>
		</div>
	</div>
</section>

<section class="page-wrapper">
	<div class="contact-section">
		<div class="container">
			<div class="contact-details col-md-12 ">
				@foreach (var item in Model)
				{
					<h2 class="mt-20 cenal cen1">@item.Title</h2>
					<div class="google-map">
						<iframe src="@item.Map" width="100%" height="300" style="border:0;" allowfullscreen="" loading="lazy"></iframe>
					</div>
				}
				<!--/. End Footer Social Links -->
			</div>
		</div>
	</div>
</section>


