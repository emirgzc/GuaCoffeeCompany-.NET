
@model IEnumerable<EntityLayer.Concrete.Setting>

@{
	ViewBag.Title = "ASetList";
	Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="col-lg-12">
	<div class="card">
		<div class="card-body">
			<h4 class="card-title">Site Ayarları</h4>
			<div class="table-responsive">
				<table class="table header-border">
					<thead>
						<tr>
							<th>#</th>
							<th>Telefon</th>
							<th>Mail</th>
							<th>Şehir</th>
							<th>Ülke</th>
							<th>Güncelle</th>
						</tr>
					</thead>
					<tbody>
						@foreach (var item in Model)
						{
							<tr>
								<td>#</td>
								<td>@item.Phone</td>
								<td>@item.Email</td>
								<td>@item.City</td>
								<td>@item.Country</td>
								<td><a href="/Setting/ASettingUpdate/@item.SetID" class="btn btn-success">Güncelle</a></td>
							</tr>

						}
					</tbody>
				</table>
			</div>
		</div>
	</div>
</div>

