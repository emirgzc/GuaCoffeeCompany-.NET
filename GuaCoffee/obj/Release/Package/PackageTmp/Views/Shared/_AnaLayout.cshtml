
@{
	ViewBag.Title = "_AnaLayout";
}

<!DOCTYPE html>
<html lang="tr">
<head>

	<meta charset="utf-8">
	<title>Gua Coffee Company ©</title>

	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="description" content="Gua Coffee Company. You have story, we have freshly brewed coffee.In Gua">
	<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=5.0">
	<meta name="author" content="Gua Coffee Company">
	<meta name="generator" content="Gua Coffee Company">
	<meta name="keywords" content="Gua Coffee Company, Kahve, Yeni Nesil Kahve"/>

	<!-- Favicon -->
	<link rel="shortcut icon" type="~/aviato/image/x-icon" href="~/aviato/images/favi.png" />

	<!-- Themefisher Icon font -->
	<link rel="stylesheet" href="~/aviato/plugins/themefisher-font/style.css">
	<!-- bootstrap.min css -->
	<link rel="stylesheet" href="~/aviato/plugins/bootstrap/css/bootstrap.min.css">

	<!-- Animate css -->
	<link rel="stylesheet" href="~/aviato/plugins/animate/animate.css">
	<!-- Slick Carousel -->
	<link rel="stylesheet" href="~/aviato/plugins/slick/slick.css">
	<link rel="stylesheet" href="~/aviato/plugins/slick/slick-theme.css">

	<!-- Main Stylesheet -->
	<link rel="stylesheet" href="~/aviato/css/style.css">

</head>

<body id="body">

	<!-- Start Top Header Bar -->
	<section class="top-header">
		<div class="container">
			<div class="row">
				@Html.Action("TopBarSetting","Setting")
			</div>
		</div>
	</section><!-- End Top Header Bar -->
	<!-- Main Menu Section -->
	<section class="menu">
		<nav class="navbar navigation">
			<div class="container">
				<div class="navbar-header">
					<h2 class="menu-title">Menü</h2>
					<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar"
							aria-expanded="false" aria-controls="navbar">
						<span class="sr-only">Menü</span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
					</button>

				</div><!-- / .navbar-header -->
				<!-- Navbar Links -->
				<div id="navbar" class="navbar-collapse collapse text-center">
					<ul class="nav navbar-nav">
						<li class="dropdown ">
							<a href="/About/Index/">Hakkımızda</a>
						</li><!-- / Home -->
						<li class="dropdown ">
							<a href="/Menu/Index/">Gua Menü</a>
						</li><!-- / Home -->
						<li class="dropdown ">
							<a href="/Franchise/Index/">Franchise</a>
						</li><!-- / Home -->
						<li class="dropdown ">
							<a href="/Kariyer/Index/">Kariyer</a>
						</li><!-- / Home -->
						<li class="dropdown ">
							<a href="/Blog/Index/">Gua Blog</a>
						</li><!-- / Home -->
						<li class="dropdown dropdown-slide">
							<a href="#!" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-delay="350"
							   role="button" aria-haspopup="true" aria-expanded="false">
								Galeri <span class="tf-ion-ios-arrow-down"></span>
							</a>
							<ul class="dropdown-menu">
								<li class="dropdown ">
									<a href="/Galery/Index/">Photos</a>
								</li><!-- / Home -->
								<li class="dropdown ">
									<a href="/Video/Index/">Vıdeos</a>
								</li><!-- / Home -->
							</ul>
						</li>
						<li class="dropdown ">
							<a href="/Activity/Index/">Etkinlikler</a>
						</li><!-- / Home -->
						<li class="dropdown ">
							<a href="/Shop/Index/">Mağaza</a>
						</li><!-- / Home -->
						<li class="dropdown ">
							<a href="/Sube/Index/">Şubelerimiz</a>
						</li><!-- / Home -->
						<li class="dropdown ">
							<a href="/Contact/Index/">İletişim</a>
						</li><!-- / Home -->
					</ul><!-- / .nav .navbar-nav -->

				</div>
				<!--/.navbar-collapse -->
			</div><!-- / .container -->
		</nav>
	</section>

	@RenderBody()

	<footer class="footer section text-center">
		<div class="container">
			<div class="row">
				<div class="col-md-12">
					@Html.Action("Footer","Slider")
					<ul class="footer-menu text-uppercase">
						<li>
							<a href="/Contact/Index/">İletişim</a>
						</li>
						<li>
							<a href="/Shop/Index/">Mağaza</a>
						</li>
						<li>
							<a href="/Sube/Index/">Şubelerimiz</a>
						</li>
						<li>
							<a href="/Blog/Index/">Gua Blog</a>
						</li>
					</ul>
					<p class="copyright-text">2021 | GUA COFFEE COMPANY © TÜM HAKLARI SAKLIDIR. Tasarım ve Geliştirme <a href="https://www.linkedin.com/in/muhammed-emir-gozcu/" rel="noopener">Emir Gözcü</a></p>
				</div>
			</div>
		</div>
	</footer>

	<!-- Main jQuery -->
	<script src="~/aviato/plugins/jquery/dist/jquery.min.js"></script>
	<!-- Bootstrap 3.1 -->
	<script src="~/aviato/plugins/bootstrap/js/bootstrap.min.js"></script>
	<!-- Bootstrap Touchpin -->
	<script src="~/aviato/plugins/bootstrap-touchspin/dist/jquery.bootstrap-touchspin.min.js"></script>
	<!-- Instagram Feed Js -->
	<script src="~/aviato/plugins/instafeed/instafeed.min.js"></script>
	<!-- Video Lightbox Plugin -->
	<script src="~/aviato/plugins/ekko-lightbox/dist/ekko-lightbox.min.js"></script>
	<!-- Count Down Js -->
	<script src="~/aviato/plugins/syo-timer/build/jquery.syotimer.min.js"></script>

	<!-- slick Carousel -->
	<script src="~/aviato/plugins/slick/slick.min.js"></script>
	<script src="~/aviato/plugins/slick/slick-animation.min.js"></script>

	<!-- Google Mapl -->
	<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCC72vZw-6tGqFyRhhg5CkF2fqfILn2Tsw"></script>
	<script type="text/javascript" src="~/aviato/plugins/google-map/gmap.js"></script>

	<!-- Main Js File -->
	<script src="~/aviato/js/script.js"></script>



</body>
</html>


