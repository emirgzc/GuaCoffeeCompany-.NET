
@model IEnumerable<EntityLayer.Concrete.Galery>

@{
	ViewBag.Title = "Index";
	Layout = "~/Views/Shared/_AnaLayout.cshtml";
}

<section class="page-header">
	<div class="container">
		<div class="row">
			<div class="col-md-12">
				<div class="content">
					<h1 class="page-name">PHOTOS</h1>
					<ol class="breadcrumb">
						<li><a href="/Slider/Index/">Anasayfa</a></li>
						<li class="active">Photos</li>
					</ol>
				</div>
			</div>
		</div>
	</div>
</section>


<section class="products section">
	<div class="container">
		<div class="row">

			@foreach (var item in Model)
			{
				<div class="col-md-3">
					<div class="product-item">
						<div class="product-thumb">
							<span class="bage">@item.Title</span>
							<img class="img-responsive" src="@item.Image" alt="Gua Coffee Company" style="width:100%; height:240px; object-fit:cover;" />
							<div class="preview-meta">
								<ul>
									<li>
										<span data-toggle="modal" data-target="#product-modal">
											<i class="tf-ion-ios-search-strong"></i>
										</span>
									</li>
								</ul>
							</div>
						</div>
					</div>
				</div>
			}

		</div>
	</div>
</section>

