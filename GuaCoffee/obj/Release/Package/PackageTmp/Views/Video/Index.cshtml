
@model IEnumerable<EntityLayer.Concrete.Video>

@{
	ViewBag.Title = "Index";
	Layout = "~/Views/Shared/_AnaLayout.cshtml";
}

<section class="page-header">
	<div class="container">
		<div class="row">
			<div class="col-md-12">
				<div class="content">
					<h1 class="page-name">VIDEOS</h1>
					<ol class="breadcrumb">
						<li><a href="/Slider/Index/">Anasayfa</a></li>
						<li class="active">Vıdeos</li>
					</ol>
				</div>
			</div>
		</div>
	</div>
</section>


<section class="products section">
	<div class="container">
		<div class="row">
			@foreach (var item in Model)
			{
				<div class="col-md-4 col-6">
					<iframe width="100%" height="250" style="object-fit:cover" src="@item.VideoURL"
							title="YouTube video player" frameborder="0"
							allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
							allowfullscreen></iframe>
				</div>
			}
		</div>
	</div>
</section>

